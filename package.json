{
  "name": "turbo-broccoli-landing",
  "version": "1.0.0",
  "description": "High-converting stock market education landing page",
  "scripts": {
    "dev": "npm run watch:css & python3 -m http.server 8000",
    "build:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --minify",
    "watch:css": "tailwindcss -i ./src/input.css -o ./dist/output.css --watch"
  },
  "devDependencies": {
    "tailwindcss": "^3.4.0",
    "daisyui": "^4.6.0"
  }
}
